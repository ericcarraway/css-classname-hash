{
  "name": "@ericcarraway/css-classname-hash",
  "author": "Eric Carraway",
  "version": "0.0.1",
  "description": "opinionated string hashing function for minifying CSS class names in Node.js, intended for use in front-end build systems",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "clear_jest": "jest --clearCache",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "pre-publish": "bash scripts/pre-publish.sh",
    "shipit": "bash scripts/publish.sh",
    "size": "size-limit",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/css-classname-hash.esm.js",
  "size-limit": [
    {
      "path": "dist/css-classname-hash.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/css-classname-hash.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.10.1",
    "husky": "^5.2.0",
    "size-limit": "^4.10.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericcarraway/css-classname-hash.git"
  },
  "bugs": {
    "url": "https://github.com/ericcarraway/css-classname-hash/issues"
  },
  "homepage": "https://github.com/ericcarraway/css-classname-hash#readme"
}
